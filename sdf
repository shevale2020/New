    <script src="<%= Page.ResolveUrl("~/Content/assets/vendor/bootstrap-datepicker/bootstrap-datepicker.js")%>"></script>
    <script type="text/javascript">
        //$("#txtWarrantyStart").datepicker({
        //    onChangeDateTime: GetAgeSpend
        //});


        $('#txtWarrantyStart').datepicker().on('changeDate', function (ev) {
            $('#txtWarrantyStart').change();
            $(this).datepicker('hide');
        });

        $('#txtWarrantyStart').change(function () {
            GetAgeSpend();
        });

        function getAge(date_1, date_2) {

            //alert(date_1);

            //alert(date_2);

            //convert to UTC

            var date2_UTC = new Date(Date.UTC(date_2.getUTCFullYear(), date_2.getUTCMonth(), date_2.getUTCDate()));

            var date1_UTC = new Date(Date.UTC(date_1.getUTCFullYear(), date_1.getUTCMonth(), date_1.getUTCDate()));

            var tempdate = date2_UTC;

            if (date2_UTC < date1_UTC) {

                date2_UTC = date1_UTC;

                date1_UTC = tempdate;

            }

            var yAppendix, mAppendix, dAppendix;





            //--------------------------------------------------------------

            var days = date2_UTC.getDate() - date1_UTC.getDate();

            if (days < 0) {



                date2_UTC.setMonth(date2_UTC.getMonth() - 1);

                days += DaysInMonth(date2_UTC);

            }

            //--------------------------------------------------------------

            var months = date2_UTC.getMonth() - date1_UTC.getMonth();

            if (months < 0) {

                date2_UTC.setFullYear(date2_UTC.getFullYear() - 1);

                months += 12;

            }

            //--------------------------------------------------------------

            var years = date2_UTC.getFullYear() - date1_UTC.getFullYear();









            if (years > 1) yAppendix = " years";

            else yAppendix = " year";

            if (months > 1) mAppendix = " months";

            else mAppendix = " month";

            if (days > 1) dAppendix = " days";

            else dAppendix = " day";

            //  alert(years + yAppendix + ", " + months + mAppendix + ", and " + days + dAppendix + " old.");



            //  return years + yAppendix + ", " + months + mAppendix + ", and " + days + dAppendix + " old.";

            $('#txtAgeSpend').val(years + yAppendix + ", " + months + mAppendix + ", and " + days + dAppendix + " old.");

        }





        function DaysInMonth(date2_UTC) {

            var monthStart = new Date(date2_UTC.getFullYear(), date2_UTC.getMonth(), 1);

            var monthEnd = new Date(date2_UTC.getFullYear(), date2_UTC.getMonth() + 1, 1);

            var monthLength = (monthEnd - monthStart) / (1000 * 60 * 60 * 24);

            return monthLength;

        }


        function GetAgeSpend() {
            var d = new Date();
            var SDate = $.trim($('#txtWarrantyStart').val());//"08/12/2017" ;//$("[id$=txtStartDate]").val().split('-');
            //alert('fghfgh1 : ' + d);
            //alert('fghfgh : ' + SDate);
            getAge(d, new Date(SDate));


        }
        //function GetAgeSpend() {
        //    var d = new Date();

        //    var SDate = $.trim($('#txtWarrantyStart').val());//"08/12/2017" ;//$("[id$=txtStartDate]").val().split('-');
        //    SDate = SDate.split('/');
        //    var Smonth = SDate[0];
